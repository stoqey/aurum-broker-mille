  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/dotenv": "^8.2.0",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.0.8",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
